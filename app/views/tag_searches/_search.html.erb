<% if user_signed_in? %>
  <div class="search_form">
    <table align="center">
      <tr>
        <div class="search_form">
          <%= form_with url: tag_searches_search_path, method: :get, local: true do |f| %>
            <td><%= f.text_field :content %></td>
            <td><%= f.submit 'タグ検索' %></td>
          <% end %>
        </div>
      </tr>
    </table>
  </div>
<% end %>
